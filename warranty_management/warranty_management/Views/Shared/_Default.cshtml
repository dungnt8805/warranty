<!DOCTYPE html>
<html lang="vi">
<head>
    <title>@ViewBag.Title :: iTD Warranty Management</title>
    <meta http-equiv="content-type" content="text/html; charset=utf-8" />
    <meta content="1800" http-equiv="refresh" />
    <meta content="width=device-width, initial-scale=1.0, user-scalable=yes" name="viewport" />
    <meta content="yes" name="apple-mobile-web-app-capable" />
    <meta name="applie-mobile-web-app-title" content="" />
    <link href="@Url.Content("~/Resources/css/bootstrap.min.css")" rel="stylesheet"/>
    <link href="@Url.Content("~/Resources/css/bootstrap-theme.css")" rel="stylesheet"/>
    <link href="@Url.Content("~/Resources/css/font-awesome.min.css")" rel="stylesheet" />
    <link href="@Url.Content("~/Resources/css/responsive.css")" rel="stylesheet"/>
    <link href="@Url.Content("~/Resources/css/style.css")" rel="stylesheet" type="text/css"/>
    <script src="@Url.Content("~/Resources/js/jquery-1.11.1.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Resources/js/bootstrap.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Resources/js/less.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Resources/js/ckeditor/ckeditor.js")" type="text/javascript"></script>
</head>
<body>
    <div class="navbar navbar-default navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <button class="navbar-toggle" data-target=".navbar-collapse" data-toggle="collapse" type="button">
                </button>
                <a class="navbar-brand site-title" href="/">iTD Warranty Management</a>
            </div>
        </div>
    </div>
    <div id="wrap">
        <div id="oshnap"></div>
        <div id="ajaxModal" class="modal fade">
            
        </div>
        <div class="container body">
            <div id="main" role="main">
                @RenderBody()
            </div>
        </div>
    </div>
    <script type="text/javascript">
        CKEDITOR.replace('textareabox', { toolbar: 'CusToolbar', height: '500px', htmlEncodeOutput: true });
    </script>
</body>
</html>
